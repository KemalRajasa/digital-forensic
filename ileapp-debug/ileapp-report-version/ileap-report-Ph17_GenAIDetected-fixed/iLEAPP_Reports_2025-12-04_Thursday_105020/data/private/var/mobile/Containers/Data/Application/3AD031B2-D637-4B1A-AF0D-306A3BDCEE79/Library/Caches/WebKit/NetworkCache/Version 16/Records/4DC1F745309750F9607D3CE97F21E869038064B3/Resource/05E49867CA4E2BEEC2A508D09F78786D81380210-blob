window.config = window.config || {};
var getYears = (i)=>{
    if (i <= 1){
    return [new Date().getFullYear().toString()];
    }
    var maxYear = getYears(--i);
    var nextYear = parseInt([...maxYear].pop()) + 1;
    maxYear.push(nextYear.toString());
    return maxYear;
};
