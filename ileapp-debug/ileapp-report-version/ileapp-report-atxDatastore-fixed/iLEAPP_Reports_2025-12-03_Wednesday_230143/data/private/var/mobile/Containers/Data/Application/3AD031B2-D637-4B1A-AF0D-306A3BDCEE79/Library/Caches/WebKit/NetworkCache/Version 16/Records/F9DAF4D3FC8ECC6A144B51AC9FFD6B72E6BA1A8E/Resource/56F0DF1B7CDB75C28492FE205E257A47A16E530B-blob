      <script>
function getCookie(name) {
  var cookies = document && document.cookie;
  var value = "; " + cookies;
  var nameString = "; " + name + "=";
  var parts = value.split(nameString);
  if (parts.length === 2) {
    return parts
      .pop()
      .split(';')
      .shift();
  }
  return '';
}

function pubsub() {
  // object which will track of all events and subscription
  var subscribers = {}
  function publish(eventName, data) {
      if(!Array.isArray(subscribers[eventName])) {
          console.warn('Event Name - ' + eventName + ' does not have any valid subscribers');
          return null;
      }
      var callbacksArr = subscribers[eventName];
      for (let i = 0; i < callbacksArr.length; i++) {
          var callback = callbacksArr[i];
          callback(data);
      }
  }
  function subscribe(eventName, callback) {
      if (!Array.isArray(subscribers[eventName])) {
          subscribers[eventName] = []
      }
      if(typeof callback === 'function') {
          subscribers[eventName].push(callback);
      } else {
          console.error('Callback provided to event name - ' + eventName + ' is not a valid function.');
          return null;
      }
      var index = subscribers[eventName] - 1;
      return function() {
          subscribers[eventName].splice(index, 1);
      }
  }
  return {
      publish,
      subscribe
  }
}
window.customPubSub = pubsub();
function setHeaders(headersObj, xhrObj) {
    var propArr = Object.keys(headersObj);
    propArr.forEach(function(prop) {
        xhrObj.setRequestHeader(prop, headersObj[prop]);
    })
}
function generateHexUID() {
    var d = new Date().getTime();
    var uid = 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function(c) {
        var r = ((d + Math.random() * 16) % 16) | 0;
        return (c === 'x' ? r : (r & 0x3) | 0x8).toString(16);
    });

    var uidOut =
        uid.substring(0, 8) + uid.substring(9, 13) + uid.substring(14, 18);
    return uidOut;
}
function initiateCartOrderAPI() {
    window.cartOrderDataFetched = false;
    var url = '/apis/stateful/v1.0/cart/current-order-details';
    var traceId = generateHexUID();
    var xhrObj = new XMLHttpRequest();
    xhrObj.open("GET", url);
    xhrObj.timeout = "30000";
    var headers = {
        'x-bbb-site-id': "BedBathUS",
        isPreview: false,
        'atg-rest-depth': 5,
        _dynSessConf: getCookie('dynSessionConfNumber'),
        'Content-Type': 'application/x-www-form-urlencoded',
        Accept: 'application/json, text/plain, */*',
        'X-B3-TraceId': traceId,
        'x-b3-spanid': traceId,
    }

    setHeaders(headers, xhrObj);

    xhrObj.onload = function responsehandler() {
        var response = JSON.parse(xhrObj.response);
        var statusCode = xhrObj.status;
        window.cartOrderData = response;
        window.cartOrderStatus = response.errorMessages && response.errorMessages[0].code || null;
        window.customPubSub.publish('cartDataLoaded');
    }

    xhrObj.send();
}

initiateCartOrderAPI();
</script>

      
