      (function () {
        window.addEventListener('DOMContentLoaded', function (event) {
          var cssFilesForMediaUpdate = document.querySelectorAll('link[identifier]');
          if(cssFilesForMediaUpdate && cssFilesForMediaUpdate.length) {
            for(var i=0; i<cssFilesForMediaUpdate.length; i++) {
                if(cssFilesForMediaUpdate[i].media !== 'all') {
                  cssFilesForMediaUpdate[i].media = 'all';
                }
            }
          }
        });
      }());
